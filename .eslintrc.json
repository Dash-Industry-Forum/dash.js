{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2015
  },
  "env": {
    "browser": true,
    "jquery": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "it": "readonly",
    "describe": "readonly",
    "beforeEach": "readonly",
    "before": "readonly",
    "afterEach": "readonly",
    "after": "readonly",
    "kendo": "readonly",
    "utils": "readonly",
    "MediaPlayer": "readonly",
    "Dash": "readonly",
    "WebKitMediaSource": "readonly",
    "MediaSource": "readonly",
    "WebKitMediaKeys": "readonly",
    "MSMediaKeys": "readonly",
    "MediaKeys": "readonly",
    "Caster": "readonly",
    "TextTrackCue": "readonly",
    "HTMLMediaElement": "readonly",
    "MediaError": "readonly",
    "cea608parser": "readonly",
    "dashjs": "readonly"
  },
  "rules": {
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "space-infix-ops": 2,
    "space-before-blocks": [
      2,
      "always"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "one-var": [
      2,
      {
        "uninitialized": "always",
        "initialized": "never"
      }
    ],
    "no-empty": [
      2,
      {
        "allowEmptyCatch": true
      }
    ],
    "no-caller": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": 0,
    "no-loop-func": 2
  }
}
