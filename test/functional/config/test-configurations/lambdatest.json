{
  "type": "lambdatest",
  "browsers": [
    "chrome_win11"
  ],
  "customLaunchers": {
    "chrome_win11": {
      "base": "WebDriver",
      "browserName": "chrome",
      "platform": "windows 11",
      "version": "latest",
      "name": "Chrome Win 11",
      "pseudoActivityInterval": 5000,
      "flags": [
        "--disable-web-security",
        "--autoplay-policy=no-user-gesture-required",
        "--disable-popup-blocking"
      ]
    }
  },
  "reporters": [
    "mocha",
    "junit",
    "coverage"
  ]
}
