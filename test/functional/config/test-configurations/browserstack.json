{
  "hostname": "localhost",
  "port": 4200,
  "protocol": "http:",
  "platform": "browserstack",
  "browsers": [
    {
      "name": "chrome",
      "os": "Windows",
      "osVersion": "10",
      "enabled": true,
      "flags": [
        "--disable-web-security",
        "--autoplay-policy=no-user-gesture-required",
        "--disable-popup-blocking",
        "--disable-search-engine-choice-screen"
      ]
    }
  ],
  "reporters": [
    {
      "name": "default",
      "options": {
        "reportTestResults": false,
        "reportTestProgress": true
      }
    },
    {
      "name": "junit",
      "options": {
        "reportLogs": true
      }
    },
    {
      "name": "summary",
      "options": {
        "flatten": false
      }
    }
  ]
}
